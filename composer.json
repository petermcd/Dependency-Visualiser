{
  "name": "rockprofile/dependency-visualiser",
  "description": "Dependency visualiser for composer based projects",
  "minimum-stability": "stable",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/petermcd/DependencyVisualiser",
  "support": {
    "issues": "https://github.com/petermcd/DependencyVisualiser/issues"
  },
  "version": "1.0.0",
  "type": "library",
  "keywords": ["dependency"],
  "autoload": {
    "psr-4": {
      "RockProfile\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Peter McDonald",
      "homepage": "https://petermcdonald.co.uk",
      "role": "Developer"
    }
