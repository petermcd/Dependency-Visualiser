  "require": {
    "php": ">=7.3.0",
    "ext-json": "*",
    "rockprofile/storage": "1.x"
  },
  "require-dev": {
    "phpstan/phpstan": "0.11.7",
    "infection/infection": "0.12.*",
    "rockprofile/dependency-visualiser-neo4j": "dev-master"
  }
}
